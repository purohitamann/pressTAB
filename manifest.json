{
    "manifest_version": 3,
    "name": "AI Email Autocomplete",
    "version": "1.0",
    "description": "Use AI to autocomplete Gmail emails using GROQ",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["https://mail.google.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
       
      }
    }
  }
  